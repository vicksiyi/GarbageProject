.time-color {
    width: 50%;
    margin: 30rpx auto;
    font-size: 1.5rem;
    text-align: center;
    color: #19be6b;
}

swiper {
    height: 100%;
}

page {
    background: #f8f8f9;
}

.title-block {
    background: #fff;
    width: 86%;
    margin: auto;
    height: 880rpx;
    border-radius: 10rpx;
}

.title-block-padding {
    width: 96%;
    margin: 20rpx auto;
}

.title-block-title {
    padding: 30rpx;
    display: flex;
    flex-direction: row;
    font-size: 0.8rem;
    font-weight: bolder;
    color: #80848f;
}

.title-block-text {
    position: relative;
    left: 386rpx;
    font-weight: normal;
}

.title-block-text text {
    color: #19be6b;
}

.title-name {
    text-align: center;
    font-weight: bolder;
    color: #495060;
    font-size: 1.2rem;
}

.title-pro {
    text-align: center;
    font-size: 0.9rem;
    margin-top: 20rpx;
    font-weight: bolder;
    color: #80848f;
}

.select .select-title {
    padding: 20rpx;
    font-size: 0.8rem;
    color: #bbbec4;
}

.select-content {
    width: 96%;
    height: 100rpx;
    background: #e9eaec;
    margin: 16rpx auto;
    border-radius: 46rpx;
    display: flex;
    flex-direction: row;
    color: #80848f;
}

.logo {
    width: 80rpx;
    height: 80rpx;
    background: #2d8cf0;
    border-radius: 50%;
    margin: 10rpx 30rpx;
}

.logo image {
    width: 60rpx;
    height: 60rpx;
    margin: 10rpx;
}


.select-content-text {
    width: 100%;
    padding-left: 88rpx;
    line-height: 108rpx;
    font-size: 1rem;
}

.select0:hover {
    background: #2d8cf0;
    color: #fff;
}

.select1:hover {
    background: #ed3f14;
    color: #fff;
}

.select2:hover {
    background: #664035;
    color: #fff;
}

.select3:hover {
    background: #495060;
    color: #fff;
}

.select4:hover {
    background: #ff9900;
    color: #fff;
}


.model-title {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0px;
    top: 0px;
    background: rgba(0, 0, 0, 0.4);
}


/* 弹窗 */
.model-title-padding {
    width: 86%;
    background: #fff;
    margin: 10% auto;
}

.model-padding-content {
    padding: 30rpx;
}

.padding-title {
    font-weight: bolder;
    font-size: 1rem;
    text-align: center;
}

.padding-num {
    text-align: center;
    font-size: 1.4rem;
    font-weight: bolder;
    padding-top: 20rpx;
}

.padding-num text {
    font-size: 1.6rem;
    color: #ff9900;
}

.padding-name {
    text-align: center;
    font-size: 0.8rem;
    padding-top: 30rpx;
    color: #666;
}

.padding-name text {
    font-size: 0.9rem;
    color: #d85600;
}

.padding-max {
    font-size: 0.8rem;
    color: #bbbec4;
    text-align: center;
    padding-top: 10rpx;
}

.padding-img {
    width: 96%;
    height: 200rpx;
    margin: 30rpx auto;
    text-align: center
}

.padding-img swiper {
    width: 100%;
    height: 200rpx;
}


.padding-img-01 {
    width: 200rpx;
    height: 200rpx;
    margin: auto;
}

.padding-img-01 image {
    width: 200rpx;
    height: 200rpx;
}
.padding-swiper-block{
    overflow: scroll;
    font-size: 0.8rem;
    height: 200rpx;
}
.padding-win {
    display: flex;
    flex-direction: row;
    font-size: 0.8rem;
    background-clip: content-box;
    color: #495060;
}

.padding-win view {
    width: 33%;
    text-align: center;
}

.padding-win view text {
    font-size: 0.9rem;
    color: #d85600;
}

.padding-row-botton {
    display: flex;
    flex-direction: row;
    margin: auto;
}

.padding-row-botton view {
    width: 50%;
    margin: auto;
}



.tr {
    display: flex;
    width: 100%;
    justify-content: center;
    height: 2rem;
    align-items: center;
}

.td {
    width: 40%;
    justify-content: center;
    text-align: center;
}

.bg-w {
    background: snow;
}

.bg-g {
    background: #E6F3F9;
    font-size: 0.8rem;
}

.th {
    width: 40%;
    justify-content: center;
    background: #2d8cf0;
    color: #fff;
    display: flex;
    height: 2rem;
    align-items: center;
}

.th.th1 {
    width: 20%;
}

.td.td1 {
    width: 20%;
    white-space:nowrap;
}

.scrol-view{
    font-size: 0.7rem;
    color: #e9eaec;
    padding-left:20rpx;
    padding-bottom: 10rpx;
}

.model-close{
    position: relative;
    left: 20rpx;
}